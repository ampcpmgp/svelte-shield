{
  "name": "Svelte Shield 拡張機能",
  "description": "Svelte Shield をそのページ内で読めるようにした拡張機能です。",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.umd.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": ["content/style.css"]
    }
  ],
  "icons": {
    "16": "/favicon.png",
    "32": "/favicon.png",
    "48": "/favicon.png",
    "128": "/favicon.png"
  },
  "permissions": ["storage", "activeTab", "scripting", "tabs", "contextMenus"]
}
