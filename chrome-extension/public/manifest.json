{
  "name": "Svelte Shield 拡張機能",
  "description": "様々な日本語文章を、目線移動無くサクサク読めるようにしたツール。",
  "version": "1.7",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.umd.js"
  },
  "icons": {
    "16": "/favicon.png",
    "32": "/favicon.png",
    "48": "/favicon.png",
    "128": "/favicon.png"
  },
  "permissions": ["storage", "activeTab", "scripting", "contextMenus"]
}
